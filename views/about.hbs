<div class="about-us-area pad-head bg-about">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="about-content">
                    <div class="section-title text-center">
                        <h2>About Us</h2>
                    </div>
                    <ol class="breadcrumb">
                        <li><a href="/">Home</a></li>
                        <li>|</li>
                        <li>About us</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="our-sponsers-area pad100">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title text-center">
                    <div class="title-text mb50">
                        <h2>Our Sponsers</h2>
                        <br>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                {{!-- sponsor start --}}
                {{#each sponsers}}
                <div class="single-sponsers  xs-mb30">
                    <ul>
                        {{#each this}}
                        <li class="xs-mb40"><img height="70" src="assets/img/sponsers/{{this}}.png" alt=""></li>
                        {{/each}}
                    </ul>
                </div>
                {{/each}}
                {{!-- sponsor end --}}
            </div>
        </div>
    </div>
</div>
<div class="pad100" >
    <div class="container" style="display:flex;">
        <div class="primary-btn" style="margin: 0 auto;">
            <a href="/contact-us" class="btn-primary">Get in touch</a>
        </div>
    </div>
</div>
<div class="counter-up-area pad100 bg-counter">
    <div class="container">
        <div class="row">
            <div class="col-xl-3 col-lg-3 col-md-4 col-sm-3">
                <div class="single-counter xs-mb40">
                    <div class="count-content">
                        <span class="count">70</span>
                        <p>Members</p>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-lg-3 col-md-4 col-sm-3">
                <div class="single-counter xs-mb40">
                    <div class="count-content">
                        <span class="count">15</span>
                        <p>Events</p>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-lg-3 col-md-4 col-sm-3">
                <div class="single-counter xs-mb40">
                    <div class="count-content">
                        <span class="count">3</span>
                        <p>Days</p>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-lg-3 d-md-none d-lg-block col-sm-3">
                <div class="single-counter">
                    <div class="count-content">
                        <span class="count">800</span>
                        <p>Participants</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>